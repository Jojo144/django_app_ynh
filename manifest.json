{
    "name": "Application Django",
    "id": "django_app",
    "packaging_format": 1,
    "version": "2019-03-10",
    "description": {
        "en": "A django app.",
        "fr": "Une application django."
    },
    "license": "WTFPL-2.0",
    "maintainer": {
        "name": "jojo144",
        "email": "jojo144@girole.fr",
        "url": "https://github.com/Jojo144/ynh_django_app"
    },
    "url": "https://github.com/Jojo144/ynh_django_app",
    "requirements": {
        "yunohost": ">> 3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for your Django app",
                    "fr": "Choisissez un nom de domaine pour vorte application Django"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for your Django app",
                    "fr": "Choisissez l'adresse pour votre application Django"
                },
                "example": "/django_app",
                "default": "/django_app"
            },
            {
                "name": "admin",
                "type": "user",
                "ask": {
                    "en": "Choose the administrator of your app (must be an existing YunoHost user)",
                    "fr": "Choisissez l'administrateur de votre application (doit Ãªtre un utilisateur YunoHost)"
                },
                "example": "johndoe"
            },
            // {
            //     "name": "email",
            //     "type": "email",
            //     "ask": {
            //         "en": "Choose email were send notification",
            //         "fr": "Choisissez l'email vers lequel envoyer les notifications"
            //     },
            //     "example": "admin@example.tld"
            // },
        ]
    }
}
